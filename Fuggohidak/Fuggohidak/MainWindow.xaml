<Window x:Class="Fuggohidak.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Fuggohidak"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Loaded="Window_Loaded">
    <Grid>
        <ListBox x:Name="liszt" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="34,32,524,112"/>
        <RadioButton x:Name="radio1" Content="2000 előtt épült" HorizontalAlignment="Left" Margin="54,336,0,0" VerticalAlignment="Top" Checked="radio1_Checked"/>
        <RadioButton x:Name="radio2" Content="2000-ben vagy után" HorizontalAlignment="Left" Margin="54,363,0,0" VerticalAlignment="Top" Checked="radio2_Checked"/>
        <TextBlock x:Name="szovegHelye" HorizontalAlignment="Left" Margin="54,383,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="94"/>
        <Label x:Name="label" Content="db" HorizontalAlignment="Left" Margin="196,381,0,0" VerticalAlignment="Top"/>
        <Button x:Name="button" Content="Keresés" HorizontalAlignment="Left" Margin="34,7,0,0" VerticalAlignment="Top" Click="button_Click"/>
        <Button x:Name="button_Copy" Content="Kilépés" HorizontalAlignment="Left" Margin="89,7,0,0" VerticalAlignment="Top" Click="button_Copy_Click"/>

    </Grid>
</Window>
